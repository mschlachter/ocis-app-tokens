{
  "name": "ocis-app-tokens",
  "version": "0.0.1",
  "description": "Generate and manage App Tokens for OwnCloud Infinite Scale.",
  "license": "Apache-2.0",
  "author": "ownCloud",
  "private": true,
  "homepage": "https://github.com/mschlachter/ocis-app-tokens",
  "type": "module",
  "scripts": {
    "build": "pnpm vite build",
    "build:w": "pnpm vite build --watch --mode development",
    "check:types": "vue-tsc --noEmit",
    "lint": "eslint 'src/**/*.{js,ts,vue}' --color",
    "test:unit": "NODE_OPTIONS=--unhandled-rejections=throw vitest"
  },
  "devDependencies": {
    "@ownclouders/eslint-config": "0.0.1",
    "@ownclouders/extension-sdk": "0.0.7",
    "@ownclouders/prettier-config": "0.0.1",
    "@ownclouders/tsconfig": "0.0.6",
    "@types/node": "20.16.11",
    "@vue/test-utils": "^2.4.6",
    "eslint": "8.57.1",
    "happy-dom": "^15.11.7",
    "prettier": "3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "5.6.2",
    "vite": "5.4.8",
    "vitest": "2.1.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-tsc": "2.1.6",
    "vue3-gettext": "^2.4.0"
  },
  "peerDependencies": {
    "@ownclouders/web-client": "^10.2.0",
    "@ownclouders/web-pkg": "^10.2.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "design-system*"
      ]
    }
  },
  "packageManager": "pnpm@9.12.1",
  "dependencies": {
    "javascript-time-ago": "^2.5.11"
  }
}
