{
  "name": "ocis-app-tokens",
  "version": "0.0.1",
  "description": "Generate and manage App Tokens for OwnCloud Infinite Scale.",
  "license": "Apache-2.0",
  "author": "ownCloud",
  "private": true,
  "homepage": "https://github.com/mschlachter/ocis-app-tokens",
  "type": "module",
  "scripts": {
    "build": "pnpm vite build",
    "build:w": "pnpm vite build --watch --mode development",
    "check:types": "vue-tsc --noEmit",
    "lint": "eslint '{src,tests}/**/*.{js,ts,vue}' --color",
    "test:unit": "NODE_OPTIONS=--unhandled-rejections=throw vitest --watch=false",
    "test:unit:w": "NODE_OPTIONS=--unhandled-rejections=throw vitest"
  },
  "devDependencies": {
    "@ownclouders/eslint-config": "^12.0.3",
    "@ownclouders/extension-sdk": "^12.0.3",
    "@ownclouders/prettier-config": "0.0.1",
    "@ownclouders/tsconfig": "0.0.6",
    "@ownclouders/web-test-helpers": "^12.0.3",
    "@types/node": "22.15.32",
    "@vue/test-utils": "^2.4.6",
    "eslint": "9.29.0",
    "happy-dom": "^20.0.2",
    "prettier": "3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "5.8.3",
    "vite": "6.4.1",
    "vitest": "3.2.4",
    "vitest-mock-extended": "3.1.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-tsc": "2.2.10",
    "vue3-gettext": "^2.4.0",
    "zod": "^3.23.8"
  },
  "dependencies": {
    "@ownclouders/web-client": "^12.0.3",
    "@ownclouders/web-pkg": "^12.0.3",
    "javascript-time-ago": "^2.5.11"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "design-system*"
      ]
    }
  },
  "packageManager": "pnpm@9.12.1"
}
